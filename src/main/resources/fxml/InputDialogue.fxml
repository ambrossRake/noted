<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="120.0" prefWidth="420.0"
			xmlns="http://javafx.com/javafx/8.0.172-ea">
	<Label fx:id="inputHeader"/>
	<TextField id="inputField" fx:id="fileNameTextField"/>
	<Label id="errorMessage" fx:id="errorMessage"/>
	<HBox alignment="CENTER_RIGHT">
		<Button id="cancelButton" text="Cancel"/>
		<Button id="confirmationButton" text="Create"/>
	</HBox>
</VBox>
